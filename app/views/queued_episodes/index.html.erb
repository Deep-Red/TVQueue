<h1>Hello!</h1>
<h2><%= @user.email %></h2>
<div class="container">
  <% @complete_queue.each do |ep| %>
    <h3>CQ</h3>
    <p>
    <%= "id: #{ep.episode_id}, airdate: #{ep.episode.airdate}" %>
    </p>
  <% end %>
  <% @unwatched_queue.each do |ep| %>
    <h3>UQ</h3>
    <p>
      <%= "id: #{ep.episode_id}, airdate: #{ep.episode.airdate}" %>
    </p>
  <% end %>
</div>
